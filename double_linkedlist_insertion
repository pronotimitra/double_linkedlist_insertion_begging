#include <stdio.h>
#include <conio.h>
#include <stdlib.h>
struct node
{
    int data;
    struct node *next;
    struct node *prev;
};
struct node*insert_begining(struct node*head,int data){
    struct node*ptr2=(struct node*)malloc(sizeof(struct node));
    head->prev=ptr2;
    ptr2->next=head;
    ptr2->prev=NULL;
    ptr2->data=data;
    head=ptr2;
   
    return ptr2;
    
}
void right_traversal(struct node *head,struct node *fourth){
    struct node*ptr=fourth;
    do{
        printf("elements when traversed from right%d:\n",ptr->data);
        ptr=ptr->prev;
    }
    while (ptr!=NULL);
    }
    void left_traversal(struct node *head){
    struct node*ptr=head;
    do{
        printf("elements when traversed from left%d:\n",ptr->data);
        ptr=ptr->next;
    }
    while (ptr!=NULL);
    }
int main()
{
    struct node *head;
    struct node *second;
    struct node *third;
    struct node *fourth;
    head=(struct node*)malloc(sizeof(struct node));
    second=(struct node*)malloc(sizeof(struct node));
    third=(struct node*)malloc(sizeof(struct node));
    fourth=(struct node*)malloc(sizeof(struct node));
    
    head->data = 77;
    head->next = second;
    head->prev = NULL;
    second->data = 4;
    second->next = third;
    second->prev = head;
    third->data = 9;
    third->next = fourth;
    third->prev = second;
    fourth->data = 14;
    fourth->next = NULL;
    fourth->prev = third;
    right_traversal(head,fourth);
    left_traversal(head);
    insert_begining(head,733);
}
